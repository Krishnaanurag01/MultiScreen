<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multiscreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultiScreen">
        <activity android:name=".IDIOMS"
            android:label="IDIOMS"></activity>
        <activity android:name=".PHRASES"
            android:label="Phrases"/>
        <activity
            android:name=".familymembers"
            android:label="Family Members" />
        <activity
            android:name=".Number"
            android:label="Number" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>